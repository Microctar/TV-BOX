		control_interval = <0xc8>;
		dvfs_tbl = <0x0c>;

		clk125_cfg {
			clk_freq = <0x7735940>;
			clk_parent = "fclk_div4";
			clkp_freq = <0x1dcd6500>;
			voltage = <0x47e>;
			keep_count = <0x05>;
			threshold = <0x1e 0x78>;
			linux,phandle = <0x02>;
			phandle = <0x02>;
		};

		clk250_cfg {
